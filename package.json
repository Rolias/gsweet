{
  "name": "gsweet",
  "version": "1.0.5",
  "description": "A project designed to help with writing scripts to run against gSuite ",
  "repository": {
    "type": "git",
    "url": "git://github.com/ps-dev/gsuite-helper.git"
  },
  "main": "./src/main.js",
  "types": "./src/gsweetTypes.d.ts",
  "scripts": {
    "test": "mocha  --use_strict   \"src/**/*.test.js\"",
    "test:int": "mocha --use_strict --reporter list \"src/**/*.testi.js\" ",
    "test:all": "run-p test   test:int",
    "start": "node --use_strict ./src/main.js",
    "lint": "eslint src --color",
    "doc": "./node_modules/.bin/jsdoc -c ./jsdoc.config.json src/**/*.js ./ReadMe.md"
  },
  "author": "Tod Gentille",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "env-create": "^1.0.3",
    "google-auth-library": "^3.1.0",
    "googleapis": "^37.2.0",
    "winston": "^3.2.1"
  },
  "keywords": [
    "gsuite",
    "gsweet",
    "sheet api",
    "doc api",
    "drive api",
    "drive",
    "spreadsheet",
    "sheet",
    "api",
    "gsuite helper",
    "google office",
    "google suite",
    "google"
  ],
  "devDependencies": {
    "@types/gapi": "0.0.36",
    "@types/node": "^11.10.5",
    "chai": "^4.2.0",
    "eslint": "^5.15.1",
    "jsdoc": "^3.5.5",
    "mocha": "^6.0.2",
    "npm-run-all": "^4.1.5",
    "sinon": "^7.2.7"
  }
}