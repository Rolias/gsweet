{
  "name": "gsuite-helper",
  "version": "1.0.0",
  "description": "A project designed to help with writing scripts to run against gSuite ",
  "main": "main.js",
  "scripts": {
    "test": "mocha  --use_strict  \"src/**/*.test.js\"",
    "testi": "mocha --use_strict --reporter list \"src/**/*.testi.js\"",
    "start": "node --use_strict ./src/main.js",
    "lint": "eslint src --color",
    "doc": "./node_modules/.bin/jsdoc src/**/*.js"
  },
  "author": "Tod Gentille",
  "license": "ISC",
  "dependencies": {
    "google-auth-library": "^3.1.0",
    "googleapis": "^37.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/gapi": "0.0.36",
    "@types/node": "^11.9.5",
    "chai": "^4.2.0",
    "eslint": "^5.14.1",
    "jsdoc": "^3.5.5",
    "mocha": "^6.0.2",
    "sinon": "^7.2.5"
  }
}